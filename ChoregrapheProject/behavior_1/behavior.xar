        targetTf  = almath.Transform(self.currentTf)
        targetTf *= almath.Transform(-dx, 0.0, dz)
        targetTf *= almath.Transform().fromRotY(dwy)
        path.append(list(targetTf.toVector()))

        return path

    def onInput_onStart(self):

